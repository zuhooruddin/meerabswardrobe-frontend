{
  "name": "meerabs_wardrobe_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev -p 4000",
    "build": "set NODE_OPTIONS=--max-old-space-size=4096 && next build",
    "build:analyze": "set ANALYZE=true && set NODE_OPTIONS=--max-old-space-size=4096 && next build",
    "export": "NODE_OPTIONS='--max-old-space-size=4096' next build && next export",
    "build:local": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "size": "node --max_old_space_size=4096",
    "fix:prettier": "prettier --write .",
    "lint": "next lint",
    "cleanup:check": "node script.js",
    "cleanup:remove": "node script.js --delete"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.4",
    "@mui/lab": "^5.0.0-alpha.51",
    "@mui/material": "^5.0.4",
    "@stripe/stripe-js": "^2.1.5",
    "apexcharts": "^3.35.2",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "currency.js": "^2.0.4",
    "date-fns": "^2.25.0",
    "formik": "^2.2.9",
    "merge": "^2.1.1",
    "next": "^12.2.5",
    "next-auth": "^4.23.1",
    "nprogress": "^0.2.0",
    "pure-react-carousel": "^1.27.6",
    "react": "18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^11.4.2",
    "react-floating-whatsapp": "^5.0.8",
    "react-modal": "^3.16.3",
    "react-quill": "^2.0.0",
    "react-scroll": "^1.8.4",
    "react-simple-image-viewer": "^1.1.1",
    "react-toastify": "^9.1.1",
    "simplebar-react": "^2.3.6",
    "stripe": "^13.6.0",
    "stylis": "^4.1.1",
    "stylis-plugin-rtl": "^2.1.1",
    "swr": "^1.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.5",
    "@types/react": "^18.0.0",
    "critters": "^0.0.23",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^2.3.2",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@types/react": "^18.0.0"
  },
  "optionalDependencies": {
    "@next/swc-win32-x64-msvc": "^12.2.5"
  },
  "browserslist": {
    "production": [
      "Chrome >= 90",
      "Firefox >= 90",
      "Safari >= 15",
      "Edge >= 90",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
